<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="PictureWrapView">
        <control type="group">
            <visible>Control.IsVisible(510)</visible>
            <include>Animation.Common</include>
            <left>0</left>
            <top>660</top>
            <control type="wraplist" id="510">
                <depth>DepthContent+</depth>
                <left>-38</left>
                <top>8</top>
                <width>1995</width>
                <height>375</height>
                <onleft>510</onleft>
                <onright>510</onright>
                <onup>2</onup>
                <ondown>60</ondown>
                <viewtype label="31032">wrap</viewtype>
                <pagecontrol>60</pagecontrol>
                <scrolltime>200</scrolltime>
                <focusposition>3</focusposition>
                <orientation>horizontal</orientation>
                <preloaditems>4</preloaditems>
                <itemlayout height="300" width="285">
                    <control type="image">
                        <left>22</left>
                        <top>60</top>
                        <width>240</width>
                        <height>240</height>
                        <aspectratio>stretch</aspectratio>
                        <bordertexture border="15">ThumbShadow.png</bordertexture>
                        <bordersize>12</bordersize>
                        <fadetime>200</fadetime>
                        <texture border="3">ThumbBG.png</texture>
                    </control>
                    <control type="image">
                        <left>38</left>
                        <top>75</top>
                        <width>210</width>
                        <height>210</height>
                        <aspectratio>keep</aspectratio>
                        <texture background="true">$INFO[ListItem.Icon]</texture>
                    </control>
                </itemlayout>
                <focusedlayout height="300" width="285">
                    <control type="image">
                        <left>22</left>
                        <top>60</top>
                        <width>240</width>
                        <height>240</height>
                        <aspectratio>stretch</aspectratio>
                        <bordertexture border="15">ThumbShadow.png</bordertexture>
                        <bordersize>12</bordersize>
                        <fadetime>200</fadetime>
                        <texture border="3">ThumbBG.png</texture>
                        <animation reversible="false" effect="zoom" start="22,60,160,160" end="-15,-8,210,210" time="150">focus</animation>
                        <animation reversible="false" effect="zoom" end="22,60,160,160" start="-15,-8,210,210" time="150">unfocus</animation>
                    </control>
                    <control type="image">
                        <left>38</left>
                        <top>75</top>
                        <width>210</width>
                        <height>210</height>
                        <aspectratio>keep</aspectratio>
                        <texture background="true">$INFO[ListItem.Icon]</texture>
                        <animation reversible="false" effect="zoom" start="38,75,140,140" end="8,15,180,180" time="150">focus</animation>
                        <animation reversible="false" effect="zoom" end="38,75,140,140" start="8,15,180,180" time="150">unfocus</animation>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="60">
                <left>465</left>
                <top>300</top>
                <width>990</width>
                <height>38</height>
                <texturesliderbackground border="15">scrollbars/scrollbaruni-bg.png</texturesliderbackground>
                <texturesliderbar border="15">scrollbars/scrollbaruni-bar.png</texturesliderbar>
                <texturesliderbarfocus colordiffuse="$VAR[ColorHighlightVar]" border="15">scrollbars/scrollbaruni-bar-focus.png</texturesliderbarfocus>
                <textureslidernib>scrollbars/scrollbaruni-nib.png</textureslidernib>
                <textureslidernibfocus>scrollbars/scrollbaruni-nib.png</textureslidernibfocus>
                <onup>510</onup>
                <ondown>2</ondown>
                <showonepage>false</showonepage>
                <orientation>horizontal</orientation>
                <visible>Control.IsVisible(510)</visible>
            </control>
        </control>
        <include>DimForContextMenu</include>
    </include>
    <include name="PictureThumbView">
        <control type="group">
            <visible>Control.IsVisible(514)</visible>
            <include>Animation.Common</include>
            <include content="NoFloorSlide">
                <param name="contentslide">60</param>
            </include>
            <control type="panel" id="514">
                <left>90</left>
                <top>62</top>
                <width>648</width>
                <height>864</height>
                <onleft>2</onleft>
                <onright>60</onright>
                <onup>514</onup>
                <ondown>514</ondown>
                <viewtype label="$LOCALIZE[31031]">list</viewtype>
                <pagecontrol>60</pagecontrol>
                <scrolltime>200</scrolltime>
                <preloaditems>2</preloaditems>
                <itemlayout height="216" width="216">
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>216</width>
                        <height>216</height>
                        <aspectratio>stretch</aspectratio>
                        <bordertexture border="15">ThumbShadow.png</bordertexture>
                        <bordersize>12</bordersize>
                        <fadetime>200</fadetime>
                        <texture border="3">ThumbBG.png</texture>
                    </control>
                    <control type="image">
                        <left>15</left>
                        <top>15</top>
                        <width>186</width>
                        <height>186</height>
                        <aspectratio>keep</aspectratio>
                        <texture background="true">$INFO[ListItem.Icon]</texture>
                    </control>
                </itemlayout>
                <focusedlayout height="216" width="216">
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>216</width>
                        <height>216</height>
                        <aspectratio>stretch</aspectratio>
                        <bordertexture colordiffuse="$VAR[ColorHighlightVar]" border="15">folder-Focus.png</bordertexture>
                        <bordersize>12</bordersize>
                        <fadetime>200</fadetime>
                        <texture border="3">ThumbBG.png</texture>
                    </control>
                    <control type="image">
                        <left>15</left>
                        <top>15</top>
                        <width>186</width>
                        <height>186</height>
                        <aspectratio>keep</aspectratio>
                        <texture background="true">$INFO[ListItem.Icon]</texture>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="60">
                <left>750</left>
                <top>45</top>
                <width>30</width>
                <height>864</height>
                <texturesliderbackground border="15">scrollbars/scrollbaruni-bg.png</texturesliderbackground>
                <texturesliderbar border="15">scrollbars/scrollbaruni-bar.png</texturesliderbar>
                <texturesliderbarfocus colordiffuse="$VAR[ColorHighlightVar]" border="15">scrollbars/scrollbaruni-bar-focus.png</texturesliderbarfocus>
                <textureslidernib>scrollbars/scrollbaruni-nib.png</textureslidernib>
                <textureslidernibfocus>scrollbars/scrollbaruni-nib.png</textureslidernibfocus>
                <onleft>514</onleft>
                <onright>2</onright>
                <showonepage>false</showonepage>
                <orientation>vertical</orientation>
                <visible>Control.IsVisible(514)</visible>
            </control>
            <control type="group">
                <left>855</left>
                <top>60</top>
                <control type="label">
                    <description>Title txt</description>
                    <left>0</left>
                    <top>0</top>
                    <width>960</width>
                    <height>22</height>
                    <label>$INFO[ListItem.Label]</label>
                    <align>center</align>
                    <aligny>center</aligny>
                    <font>font24_title</font>
                    <textcolor>white</textcolor>
                    <shadowcolor>black</shadowcolor>
                </control>
                <control type="image">
                    <left>0</left>
                    <top>45</top>
                    <width>960</width>
                    <height>705</height>
                    <texture background="true">$INFO[ListItem.FilenameAndPath]</texture>
                    <fadetime>600</fadetime>
                    <aspectratio>keep</aspectratio>
                    <bordertexture border="12">ThumbShadow.png</bordertexture>
                    <bordersize>14</bordersize>
                </control>
                <control type="label">
                    <description>Date time txt</description>
                    <left>0</left>
                    <top>765</top>
                    <width>960</width>
                    <height>22</height>
                    <label>[COLOR=$VAR[ColorHighlightVar]]$INFO[ListItem.PictureDateTime,$LOCALIZE[31326] - ][/COLOR]</label>
                    <align>left</align>
                    <aligny>center</aligny>
                    <font>font13</font>
                    <textcolor>white</textcolor>
                    <shadowcolor>black</shadowcolor>
                </control>
                <control type="label">
                    <description>Resolution txt</description>
                    <left>0</left>
                    <top>802</top>
                    <width>960</width>
                    <height>22</height>
                    <label>[COLOR=$VAR[ColorHighlightVar]]$INFO[ListItem.PictureResolution,$LOCALIZE[31327] - ][/COLOR]</label>
                    <align>left</align>
                    <aligny>center</aligny>
                    <font>font13</font>
                    <textcolor>white</textcolor>
                    <shadowcolor>black</shadowcolor>
                </control>
            </control>
        </control>
        <include>DimForContextMenu</include>
    </include>
</includes>
